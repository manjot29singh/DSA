public  class linkedlist{
    private class Node{
        int data = 0;
        Node next = null;

        Node(int data){
            this.data = data;
        }
    }
    private Node head = null;
    private Node tail = null;
    private int sizeofll = 0;

    public boolean isempty(){
        return sizeofll == 0;
    }
    
